meta {
  name: /user-profile/company-id/{companyId}
  type: http
  seq: 7
}

get {
  url: {{base_url}}/api/user-profile/company-id/2
  body: json
  auth: none
}

script:post-response {
  const response = res.getBody();
  
  test("Status code is 200", function () {
      expect(res.getStatus()).to.equal(200);
  });
  
  test("Contains Users", function () {
      expect(response.length).to.be.greaterThan(0);
  });
  
  test("No password returned", function () {
    response.forEach(user => {
      expect(user.userPassword).to.be.null;
    });  
  });
  
}

settings {
  encodeUrl: true
  timeout: 0
}
